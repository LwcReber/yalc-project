{
  "name": "my-vue-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite  --host --force",
    "watch": "nodemon --debug  --watch './.yalc/*'  -x 'npm run dev'",
    "build": "tsc && vite build",
    "add": "yalc add web-sdk-yalc --link",
    "remove": "yalc remove web-sdk-yalc",
    "preview": "vite preview"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "web-sdk-yalc": "link:.yalc/web-sdk-yalc"
  },
  "devDependencies": {
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@vitejs/plugin-react": "^3.1.0",
    "nodemon": "^2.0.22",
    "typescript": "^4.9.3",
    "vite": "^4.2.0"
  }
}
